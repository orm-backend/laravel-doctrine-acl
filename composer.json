{
    "name": "it-aces/laravel-doctrine-acl",
    "description": "Adds support for user group and enitiy permissions.",
    "type": "library",
    "license": "MIT",
    "keywords": ["it-aces", "laravel-doctrine", "acl"],
    "authors": [
        {
            "name": "Vitaliy Kovalenko",
            "email": "vvk@kola.cloud"
        }
    ],
    "require": {
    	"it-aces/laravel-doctrine": "dev-master"
